<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="?attr/selectableItemBackground" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick"
                android:text="@string/b1name"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/button2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick2"
                android:text="@string/b2name" />

            <Button
                android:id="@+id/button4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick3"
                android:text="@string/b3name" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <Button
                android:id="@+id/button6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick4"
                android:text="@string/b4name" />

            <Button
                android:id="@+id/button7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick5"
                android:text="@string/b5name" />

            <Button
                android:id="@+id/button8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="@string/b6name"
                android:text="6" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <Button
                android:id="@+id/button9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick7"
                android:text="@string/b7name" />

            <Button
                android:id="@+id/button10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick8"
                android:text="@string/b8name" />

            <Button
                android:id="@+id/button11"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick9"
                android:text="@string/b9name" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="137dp"
            android:layout_height="70dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <Button
                android:id="@+id/button12"
                android:layout_width="153dp"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClick0"
                android:text="@string/b0name" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:visibility="visible"
            tools:visibility="visible">

            <Button
                android:id="@+id/button17"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClickUmno"
                android:text="@string/bumnoname" />

            <Button
                android:id="@+id/button18"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClickRazdel"
                android:text="@string/brazdname" />

            <Button
                android:id="@+id/button15"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClickRav"
                android:text="@string/bravnoname" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button16"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClickMinus"
                android:text="@string/bminusname" />

            <Button
                android:id="@+id/button14"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClickPlus"
                android:text="@string/bplusname" />

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="?attr/dialogCornerRadius"
                android:layout_weight="1"
                android:onClick="MyClickBack"
                android:text="@string/bBackname"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>